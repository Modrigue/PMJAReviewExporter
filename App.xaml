             xmlns:local="clr-namespace:PMJAReviewExporter"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <BitmapImage x:Key="PA_Logo" UriSource="PA/PA_Logo.jpg" />
